{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "functions": {
        "index.js": {
            "maxDuration": 30
        }
    },
    "env": {
        "NODE_ENV": "production"
    },
    "installCommand": "npm install",
    "buildCommand": "echo 'No build needed'",
    "outputDirectory": "."
}